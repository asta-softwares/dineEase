{
  "expo": {
    "name": "DineEase",
    "slug": "dineease",
    "version": "0.5",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "plugins": ["react-native-edge-to-edge"],
    "splash": {
      "image": "./assets/splash-new.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ca.dineease.app",
      "icon": "./assets/logo-ios.png",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-android.png"
      },
      "package": "ca.dineease.app",
      "permissions": ["INTERNET"],
      "networkSecurityConfig": {
        "cleartextTrafficPermitted": true
      },
      "googleServicesFile": "./google-services.json",
      "softwareKeyboardLayoutMode": "pan",
      "windowSoftInputMode": "adjustResize"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "bb93df51-05e6-4f7e-b143-45bac0194bb7"
      }
    },
    "owner": "mendozarain",
    "experiments": {
      "newArchEnabled": true
    }
  }
}
