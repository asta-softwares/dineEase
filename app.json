{
  "expo": {
    "name": "DineEase",
    "slug": "dineease",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": ["react-native-edge-to-edge"],
    "splash": {
      "image": "./assets/splash-new.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ca.dineease.app",
      "buildNumber": "2",
      "icon": "./assets/logo-ios.png",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "NSCameraUsageDescription": "This app includes the ability to scan credit cards for payment, though this feature is not currently in use.",
        "config": {
          "usesNonExemptEncryption": false
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-android.png",
        "backgroundColor": "#ffffff"
      },
      "package": "ca.dineease.app",
      "versionCode": 5,
      "permissions": ["INTERNET"],
      "googleServicesFile": "./google-services.json",
      "softwareKeyboardLayoutMode": "pan",
      "networkSecurityConfig": {
        "cleartextTrafficPermitted": true
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "bb93df51-05e6-4f7e-b143-45bac0194bb7"
      }
    },
    "owner": "mendozarain"
  }
}
